{
  "name": "simian-photo-server",
  "version": "0.0.2",
  "description": "A node based photo server",
  "main": "app/index.js",
  "scripts": {
    "test": "node_modules/.bin/nyc node_modules/.bin/mocha test/**/*.js",
    "lint": "./node_modules/.bin/eslint gfiles/ src/js/ test",
    "clean": "gulp clean-all",
    "build": "gulp all",
    "clean-build": "npm run clean && npm run build",
    "start-crawler": "node crawler/crawler/index.js",
    "start-server": "node server/server/index.js",
    "start-all": "npm run start-server && npm run start-crawler",
    "document": "jsdoc src -c jsdoc.conf",
    "plato": "plato -d plato -r -e .eslintrc.plato.json src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akshat1/simian-photo-server.git"
  },
  "keywords": [
    "photos",
    "self-hosted"
  ],
  "author": "akshat.sh@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akshat1/simian-photo-server/issues"
  },
  "homepage": "https://github.com/akshat1/simian-photo-server#readme",
  "nyc": {
    "all": true,
    "include": [
      "src/js/**/*.js",
      "src/js/**/*.jsx"
    ],
    "exclude": [
      "test/**",
      "node_modules/**",
      "plato/**",
      "docs/**"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "extension": [
      ".jsx",
      ".js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.0.0",
    "del": "^2.2.2",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^3.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^3.3.0",
    "gulp-sourcemaps": "^2.6.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.3",
    "jsdoc-strip-async-await": "^0.1.0",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "mocha": "^3.4.2",
    "mockery": "^2.0.0",
    "nyc": "^10.3.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sinon": "^2.3.2",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "express": "^4.15.3",
    "fs-extra": "^3.0.1",
    "graphql": "^0.10.1",
    "imagemagick": "^0.1.3",
    "isomorphic-fetch": "^2.2.1",
    "mime": "^1.3.6",
    "mongodb": "^2.2.27",
    "nconf": "^0.8.4",
    "node-hashes": "^0.1.0",
    "node-ipc": "^9.0.1",
    "plato": "^1.7.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-promise": "^0.5.3",
    "serve-static": "^1.12.3",
    "walk": "^2.3.9",
    "winston": "^2.3.1"
  }
}
